issn = {2314-6133},
journal = {BioMed Research International},
pages = {5518999},
publisher = {Hindawi},
title = {{Synthesis and Toxicity of Graphene Oxide Nanoparticles: A Literature Review of In Vitro and In Vivo Studies}},
url = {https://doi.org/10.1155/2021/5518999},
volume = {2021},
year = {2021}
}

